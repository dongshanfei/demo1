/* pages/wx/wx.wxss */

page {
    background-color: #fff;
}

@font-face {
    font-family: "iconfont";
    src: url('iconfont.eot?t=1523870903202'); /* IE9*/
    src: url('iconfont.eot?t=1523870903202#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1523870903202') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1523870903202#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-lianjie:before {
    content: "\e665";
}

.icon-lianjie1:before {
    content: "\e661";
}

.icon-fenxiang1:before {
    content: "\e61f";
}

.icon-fenxiang:before {
    content: "\e624";
}

.icon-shouye:before {
    content: "\e6d9";
}

.icon-jiantou:before {
    content: "\e64a";
    font-size: 70rpx;
    color: #5e5e5e;
}

.content1 {
    text-align: center;
    box-sizing: border-box;
    padding-top: 100rpx;
    position: absolute;
    width: 100%;
    height: 100%;
}

.mask {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 100;
}

.mask image {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
}

.logoWrap {
    display: inline-block;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    overflow: hidden;
}

.logoWrap image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.name text {
    font-size: 30rpx;
    line-height: 60rpx;
}

.fix {
    display: inline-block;
    padding: 15rpx 40rpx;
    color: #fff;
    font-size: 35rpx;
    background-color: #169bd5;
    border-radius: 10rpx;
    margin-top: 10rpx;
}

.name {
    color: #5f5f5f;
}

.intro {
    box-sizing: border-box;
    width: 100%;
    line-height: 56rpx;
    font-size: 28rpx;
    letter-spacing: 3rpx;
    /* text-indent: 2em; */
    text-align: center;
    color: #5f5f5f;
}

.hidden {
    width: 100%;
    text-align: center;
}

.hidden image {
    width: 100%;
}

.marginTop {
    margin-top: 30rpx;
}

.gowxApp {
    padding-bottom: 50rpx;
}

.appItem image {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}

.appItem {
    width: 90%;
    margin: auto;
    margin-top: 30rpx;
    box-sizing: border-box;
    display: flex;
    padding: 40rpx;
    background-color: #f1f1f1;
    border-radius: 20rpx;
    position: relative;
    border-top: 3rpx solid #eee;
    align-items: center;
    box-shadow: 5px 5px 3px #e3e3e3;
}

.row {
    position: absolute;
    height: 60rpx;
    width: 30rpx;
    line-height: 60rpx;
    font-size: 35rpx;
    top: 0;
    bottom: 0;
    right: 45rpx;
    margin: auto;
    color: #9e9e9e;
}

.appTitle {
    font-size: 40rpx;
    display: block;
    text-align: left;
    font-weight: bold;
    letter-spacing: 3rpx;
    color: #5e5e5e;
    text-align: justify;
}

.appIntro {
    margin-left: 50rpx;
    box-sizing: border-box;
    padding-right: 20rpx;
    width: 60%;
    color: #5f5f5f;
}

.appDes {
    display: inline-block;
    font-size: 30rpx;
    text-align: left;
    color: #5f5f5f;
    margin-top: 20rpx;
}

.jitang {
    width: 100%;
    height: auto;
    margin-top: -80rpx;
}
