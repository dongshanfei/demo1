/***********2018-4-26新版**************/

.bottomFix {
    width: 100%;
    position: absolute;
    bottom: 0;
    z-index: 901;
}

.bottomSelect {
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 80rpx;
    background-color: #fff;
}

.select {
    width: 20%;
    font-size: 35rpx;
    line-height: 35rpx;
    height: 80rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.selectWrap {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.fontWrap {
    display: inline-block;
    width: 50%;
    height: 50rpx;
    text-align: center;
}

.select text {
    display: inline-block;
    font-size: 25rpx;
    color: #fff;
}

.underLine {
    width: 70%;
    margin: auto;
    border-top: 2rpx solid #fff;
    background-color: #fff;
    display: block;
    line-height: 0;
}

.bottomSelect image {
    width: 40rpx;
    height: 40rpx;
}

.borderRight {
    border-right: 3rpx solid rgb(196, 196, 196);
}

.u {
    width: 0;
    height: 0;
    border-left: 15rpx solid transparent;
    border-right: 15rpx solid transparent;
    border-bottom: 15rpx solid rgb(78, 114, 151);
    margin: 0 auto;
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    top: -14rpx;
    z-index: 100;
}

.active {
    background-color: rgb(78, 114, 151);
}

.active view {
    border: none;
}

.compWrap {
    position: absolute;
    width: 100%;
    height: 165rpx;
    bottom: 80rpx;
    left: 0;
    background-color: rgba(121, 152, 180, 0.6);
    transform: translateY(100%);
    transition-timing-function: ease-in;
    transition: all 1s;
    z-index: 900;
}

.psRow {
    position: absolute;
    top: -46rpx;
    right: 10rpx;
    width: 70rpx;
    height: 40rpx;
    text-align: center;
    background-color: rgb(78, 114, 151);
    color: #fff;
    padding-bottom: 10rpx;
}

.icon-iconfontarrows {
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    left: 0;
    right: 0;
}

@keyframes slidDown {
    from {
        transform: translateY(0%);
    }

    to {
        transform: translateY(100%);
    }
}

.slidDown {
    transform: translateY(100%);
    /* display: block;
    animation: slidDown 0.3s ease-in both; */
}

@keyframes slidup {
    from {
        transform: translateY(100%);
    }

    to {
        transform: translateY(0%);
    }
}

.slidup {
    /* display: block;
    animation: slidup 0.3s ease-in both; */
    transform: translateY(0%);
}

.warpImg {
    width: 120rpx;
    height: 120rpx;
    background-color: #fff;
    border-radius: 10rpx;
    border: 2px solid #81b7c4;
    box-shadow: 0px 0px 5px 3px #b6c7cb;
    position: relative;
}

.underline_2 {
    display: none;
}

.underline_2::after {
    content: "";
    display: inline-block;
    width: 100rpx;
    height: 4rpx;
    background-color: #fff;
    position: absolute;
    bottom: -12rpx;
    left: 0;
    right: 0;
    margin: auto;
}

.scrollView {
    position: absolute;
    top: 0;
    left: 0;
}

.touched {
    margin-top: -15rpx;
}

.touched .underline_2 {
    display: inline-block;
}

/******************/

.move {
    width: 200rpx;
    height: 200rpx;
    background-color: red;
}

.imgWrap {
    box-sizing: border-box;
    width: 100%;
    transform-origin: center;
    float: left;
    border: 5rpx transparent dashed;
}

.imgWrap image {
    float: left;
}

.touchActive {
    border: 4rpx #fff dashed;
    z-index: 400;
}

.touchActive .x {
    display: block;
}

.touchActive .o {
    display: block;
}

.x {
    position: absolute;
    top: -25rpx;
    left: -25rpx;
    z-index: 500;
    display: none;
    width: 50rpx;
    height: 50rpx;
    overflow: hidden;
    font-weight: bold;
    color: #d1e3f1;
}

/* 
.x::after {
    content: "X";
    font-size: 40rpx;
    color: red;
} */

.o {
    position: absolute;
    bottom: -25rpx;
    right: -25rpx;
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    display: none;
    overflow: hidden;
    font-weight: bold;
    color: #d1e3f1;
}

/* 
.o::after {
    content: "O";
    font-size: 40rpx;
    color: blue;
} */

.touchWrap {
    transform-origin: center;
    /* height: 200rpx; */
    position: absolute;
    z-index: 100;
}

.btnView {
    position: absolute;
    right: 15rpx;
    bottom: 350rpx;
}

.btnView button {
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    font-size: 20rpx;
    line-height: 60rpx;
    color: #fff;
    margin: 0;
    padding: 0;
    border-radius: 50%;
    background-color: rgb(78, 114, 151);
}

.btnView button::after {
    border: none;
}

.canvasWrap {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 999;
    padding-top: 10px;
}

.maskCanvas {
    margin: auto;
}

.btn {
    font-size: 30rpx;
    color: #81b7c4;
    width: 50%;
    margin-top: 20rpx;
    background-color: #fff;
    height: 90rpx;
    line-height: 90rpx;
    border: 3rpx solid #81b7c4;
}

.btnView view {
    padding-bottom: 20rpx;
}

.hand {
    position: absolute;
    left: 100rpx;
    right: 0;
    margin: auto;
    z-index: 100;
}

.getUserInfoBtn {
    position: initial;
    border: none;
    background-color: none;
}

.getUserInfoBtn::after {
    border: none;
}
